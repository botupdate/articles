Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: https://briantomasik.com/css/tooltip.css

@charset "utf-8";

:root { --tooltip_width: 130px; }

@media (min-width: 875px) {
  :root { --tooltip_width: 230px; }
}

[data-tooltip] { position: relative; z-index: 2; cursor: pointer; }

[data-tooltip]::before, [data-tooltip]::after { visibility: hidden; opacity: 0; pointer-events: none; }

[data-tooltip]::before { transition: opacity 0.5s ease 0s; position: absolute; bottom: 150%; left: 50%; margin-bottom: 5px; margin-left: calc(-1 * var(--tooltip_width) / 2); padding: 7px; width: var(--tooltip_width); border-radius: 3px; background-color: rgb(255, 252, 243); color: rgb(20, 20, 20); content: attr(data-tooltip); font-size: 70%; text-decoration: none; }

[data-tooltip]::after { position: absolute; bottom: 150%; left: 50%; margin-left: -5px; width: 0px; border-top: 5px solid rgb(146, 64, 0); border-right: 5px solid transparent; border-left: 5px solid transparent; content: " "; font-size: 0px; line-height: 0; }

[data-tooltip]:hover::before, [data-tooltip]:hover::after { visibility: visible; opacity: 1; }
