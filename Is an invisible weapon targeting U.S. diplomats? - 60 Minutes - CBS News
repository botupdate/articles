if(survey3.indexOf(&quot;http&quot;)==0){var s3=document.createElement('script'); s3.src=survey3; thirdparty_assets_el.appendChild(s3);}

/* ----- error checking ----- */
if(false){
	var errorText = &quot;&quot;;
	errorText += ads_errorCheck(trackingPixel1,&quot;Third party impression tracker 1&quot;);
	errorText += ads_errorCheck(trackingPixel2,&quot;Third party impression tracker 2&quot;);
	errorText += ads_errorCheck(trackingPixel3,&quot;Third party impression tracker 3&quot;);
	errorText += ads_errorCheck(survey1,&quot;Survey&quot;);
	errorText += ads_errorCheck(survey2,&quot;Survey / Javascript 2&quot;);
	errorText += ads_errorCheck(survey3,&quot;Survey / Javascript 2&quot;);
	if(errorText != &quot;&quot;){ alert(&quot;!!! ERROR !!!\n\nA line break or space has been detected in the following inputs:\n\n&quot;+errorText); }
}
function ads_errorCheck(str,name){if(str.search(&quot;\n&quot;)>-1 || str.search(&quot; &quot;)>-1){return &quot;- &quot;+name+&quot;\n\n&quot;;}else{return &quot;&quot;;}}
/* ----- END error checking ----- */
</script>
